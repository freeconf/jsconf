{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "lib": ["es5", "dom", "es6"],
    "declaration": true,
    "sourceMap": true,
    "inlineSources": true,
    "outDir": "build",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "rootDir": "src",
    "strict": true
  },
  "include": [
    "src/lib/**/*.ts"
  ],
  "exclude": []
}
